def find_student_by_num(number):
    exit_flag = "N"
    number = str(number)
    while exit_flag == "N" or exit_flag == "n" or exit_flag == "":        
        students_searched = 0
        ##Find all students in specified grade    
        for i in range(len(s_numbers)):        
            if s_numbers[i] == number:
                print s_numbers[i]
                print c1[i]
